---
import Layout from "../layouts/Layout.astro";
import data from "../data/notes.json";

const pageTitle = "Notes";
const heading = "Notes";
---

<Layout pageTitle={pageTitle}>
	<div class="wrapper">
		<h1>{heading}</h1>
	</div>
	<div class="wrapper region flow">
		{
			data.map((note) => {
				return (
					<>
						<h2>{note.title}</h2>
						<p>{note.description}</p>
						<p>
							<a href={`${note.url}`}>Link</a>
						</p>
					</>
				);
			})
		}
	</div>
</Layout>
